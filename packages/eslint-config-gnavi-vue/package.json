{
  "name": "eslint-config-gnavi-vue",
  "version": "0.0.0",
  "description": "ESLint config for Vue.js Project of gnavi",
  "main": "index.js",
  "scripts": {
    "start": "prettier-eslint --write \"./**/*.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "eslint",
      "git add"
    ]
  },
  "engines": {
    "node": "^10.15.1",
    "npm": "^6.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gurunavi-creators/eslint-config-gnavi.git"
  },
  "keywords": [
    "eslint",
    "vue"
  ],
  "author": "gnavi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gurunavi-creators/eslint-config-gnavi/issues"
  },
  "homepage": "https://github.com/gurunavi-creators/eslint-config-gnavi/tree/master/packages/eslint-config-gnavi-vue#readme",
  "dependencies": {
    "eslint": "5.14.1",
    "eslint-plugin-vue": "^5.2.2"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier-eslint-cli": "^4.7.1"
  },
  "peerDependencies": {
    "eslint": "5.14.1",
    "eslint-plugin-vue": "^5.2.2"
  }
}